{
    "inputs": [
        {
            "detail": {
                "Environments": [
                    "DEBUG=true"
                ],
                "IntervalMs": 100000,
                "ScriptType": "shell",
                "ScriptContent": "",
                "User": "op",
                "LineSplitSep": "\n",
                "CmdPath": "/bin/sh",
                "IgnoreError": false,
                "ContentEncoding": "Base64",
                "TimeoutMilliseconds": 30000
            },
            "type": "input_command"
        }
    ],
    "processors": [
        {
            "detail": {
                "SourceKey": "content",
                "Regex": "(\\S+) (\\d+.\\d+.\\d+.\\d+) (\\w+) (\\d.*)",
                "Keys": [
                    "hostname",
                    "ip",
                    "metrics_type",
                    "value"
                ]
            },
            "type": "processor_regex"
        },
        {
            "detail": {
                "Fields": {
                    "Metric": "op_metrics"
                }
            },
            "type": "processor_add_fields"
        },
        {
            "detail": {
                "IgnoreError": false,
                "MetricLabelKeys": [
                    "hostname",
                    "ip",
                    "metrics_type"
                ],
                "MetricValues": {
                    "Metric": "value"
                }
            },
            "type": "processor_log_to_sls_metric"
        }
    ]
}
